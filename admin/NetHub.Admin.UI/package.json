{
  "name": "nethub-admin",
  "version": "0.4.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host admin.nethub.local --port 9100 --disable-host-check",
    "build": "ng build",
    "build:ssr": "ng run nethub-admin:server:dev",
    "watch": "ng build --watch --configuration development",
    "lint": "ng lint",
    "lint:fix": "\"node_modules/.bin/eslint\" --fix .",
    "nswag": "\"node_modules/.bin/nswag\"",
    "nswag:gen": "npm run nswag run /runtime:Net70",
    "reinstall:neercore-admin": "npm install ../../../neerspace-angular/dist/neercms"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.12",
    "@angular/cdk": "^14.2.7",
    "@angular/common": "^14.2.12",
    "@angular/compiler": "^14.2.12",
    "@angular/core": "^14.2.12",
    "@angular/forms": "^14.2.12",
    "@angular/platform-browser": "^14.2.12",
    "@angular/platform-browser-dynamic": "^14.2.12",
    "@angular/platform-server": "^14.2.12",
    "@angular/router": "^14.2.12",
    "angular-split": "^14.1.0",
    "bootstrap": "^5.2.3",
    "jwt-decode": "^3.1.2",
    "neercms": "file:../../../neerspace-angular/dist/neercms",
    "oidc-client": "^1.11.5",
    "run-script-os": "^1.1.6",
    "rxjs": "~7.5.7",
    "tslib": "^2.5.0",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.9",
    "@angular-eslint/builder": "14.3.0",
    "@angular-eslint/eslint-plugin": "14.3.0",
    "@angular-eslint/eslint-plugin-template": "14.3.0",
    "@angular-eslint/schematics": "14.3.0",
    "@angular-eslint/template-parser": "14.3.0",
    "@angular/cli": "^14.2.9",
    "@angular/compiler-cli": "^14.2.11",
    "@angular/localize": "^14.2.11",
    "@types/luxon": "^3.1.0",
    "@types/node": "^18.7.11",
    "@types/vanillajs-datepicker": "^1.2.1",
    "@typescript-eslint/eslint-plugin": "5.37.0",
    "@typescript-eslint/parser": "5.37.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-no-relative-import-paths": "^1.5.2",
    "eslint-plugin-prettier": "^4.2.1",
    "nswag": "^13.17.0",
    "prettier-eslint": "^15.0.1",
    "typescript": "~4.7.4"
